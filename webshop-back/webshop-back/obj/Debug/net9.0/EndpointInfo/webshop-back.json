{
  "openapi": "3.0.1",
  "info": {
    "title": "webshop-back",
    "version": "1.0"
  },
  "paths": {
    "/api/Payments/init": {
      "post": {
        "tags": [
          "Payments"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentInitRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentInitRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentInitRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Payments/callback": {
      "post": {
        "tags": [
          "Payments"
        ],
        "parameters": [
          {
            "name": "orderId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "status",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "globalTransactionId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/psp/simulate-payment": {
      "get": {
        "tags": [
          "PspSimulator"
        ],
        "parameters": [
          {
            "name": "paymentId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "successUrl",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "failedUrl",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/psp/complete-payment": {
      "post": {
        "tags": [
          "PspSimulator"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "paymentId": {
                    "type": "string"
                  },
                  "action": {
                    "type": "string"
                  },
                  "successUrl": {
                    "type": "string"
                  },
                  "failedUrl": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "paymentId": {
                  "style": "form"
                },
                "action": {
                  "style": "form"
                },
                "successUrl": {
                  "style": "form"
                },
                "failedUrl": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Vehicles": {
      "get": {
        "tags": [
          "Vehicles"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Vehicles/{id}": {
      "get": {
        "tags": [
          "Vehicles"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PaymentInitRequest": {
        "type": "object",
        "properties": {
          "merchanT_ID": {
            "type": "string",
            "nullable": true
          },
          "merchanT_PASSWORD": {
            "type": "string",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          },
          "currency": {
            "type": "string",
            "nullable": true
          },
          "merchanT_ORDER_ID": {
            "type": "string",
            "nullable": true
          },
          "merchanT_TIMESTAMP": {
            "type": "string",
            "nullable": true
          },
          "succesS_URL": {
            "type": "string",
            "nullable": true
          },
          "faileD_URL": {
            "type": "string",
            "nullable": true
          },
          "erroR_URL": {
            "type": "string",
            "nullable": true
          },
          "method": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}